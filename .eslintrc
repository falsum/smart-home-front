{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true
  },
  "rules": {
    "react/jsx-filename-extension": 0,
    "arrow-parens": 0,
    "react/prop-types": 0,
    "import/prefer-default-export": 1
  }
}